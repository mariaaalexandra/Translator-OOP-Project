Word{word='chat', word_en'cat', type='noun', singular=[chat], plural=[chats], definitions=[Definition{dict='Larousse', dictType='synonyms', year=2000, text=[greffier, mistigri, matou, minet]}, Definition{dict='Larousse', dictType='definitions', year=2000, text=[Mammifère carnivore (félidé), sauvage ou domestique, au museau court et arrondi, Familier. Terme d'affection, de tendresse adressé à quelqu'un, Jeu d'enfants dans lequel un des joueurs, le chat, poursuit les autres et tente de les toucher]}]}
Word{word='jeu', word_en'game', type='noun', singular=[jeu], plural=[jeux], definitions=[Definition{dict='Larousse', dictType='synonyms', year=2000, text=[amusement, délassement, distraction, divertissement, passe-temps, récréation]}, Definition{dict='Larousse', dictType='definitions', year=2000, text=[Activité d'ordre physique ou mental, non imposée, ne visant à aucune fin utilitaire, et à laquelle on s'adonne pour se divertir, en tirer un plaisir, Activité de loisir soumise à des règles conventionnelles, comportant gagnant(s) et perdant(s) et où interviennent, de façon variable, les qualités physiques ou intellectuelles, l'adresse, l'habileté et le hasard, Ensemble des différents jeux de hasard, en particulier ceux où l'on risque de l'argent, Ensemble des règles qui régissent un divertissement organisé]}]}
Word{word='manger', word_en'eat', type='verb', singular=[mange, manges, mange], plural=[mangeons, mangez, mangent], definitions=[Definition{dict='Larousse', dictType='synonyms', year=2000, text=[absorber, becqueter, bouffer, boulotter, briffer, ingurgiter]}, Definition{dict='Larousse', dictType='definitions', year=2000, text=[Porter quelque chose à sa bouche, le mâcher, le mâchonner, le ronger, le mordiller, etc. (sans généralement avaler), Entamer quelque chose, ronger, altérer, En parlant d'un insecte, entamer quelque chose, une matière, y faire des trous]}]}
Word{word='pisică', word_en'cat', type='noun', singular=[pisică], plural=[pisici], definitions=[Definition{dict='Dicționar de sinonime', dictType='synonyms', year=1998, text=[mâță, cotoroabă, cătușă]}, Definition{dict='Dicționarul explicativ al limbii române, ediția a II-a', dictType='definitions', year=2002, text=[Mamifer domestic carnivor din familia felinelor, cu corpul suplu, acoperit cu blană deasă și moale de diferite culori, cu capul rotund, cu botul foarte scurt, cu maxilarele puternice și cu ghearele retractile și ascuțite, Dispozitiv de agățare și de desprindere a berbecului din capătul cablului de ridicare de la sonetele cu cădere liberă, Mănunchi de sârme de oțel, folosit pentru curățarea de noroi sau de pământ a utilajelor de foraj]}]}
Word{word='câine', word_en'dog', type='noun', singular=[câine], plural=[câini], definitions=[Definition{dict='Dicționar de sinonime', dictType='synonyms', year=1998, text=[lătrător, molie, dulău]}, Definition{dict='Dicționarul explicativ al limbii române (ediția a II-a revăzută și adăugită)', dictType='definitions', year=2009, text=[Animal mamifer carnivor, domesticit, folosit pentru pază, vânătoare etc.]}, Definition{dict='Dicționar universal al limbei române, ediția a VI-a)', dictType='definitions', year=1929, text=[animal domestic din familia carnivorelor digitigrade, fig. (termen de dispreț) om rău (câinele fiind considerat ca tipul răutății): câinele de tată-său Pop, nume a două constelațiuni, Mold. (sens tehnic) masă de tras doage]}]}
Word{word='merge', word_en'walk', type='verb', singular=[merg, mergi, merge], plural=[mergem, mergeți, merg], definitions=[Definition{dict='Dicționarul explicativ al limbii române (ediția a II-a revăzută și adăugită)', dictType='definitions', year=2009, text=[A se mișca deplasându-se dintr-un loc în altul; a se deplasa, a umbla, A putea fi înghițit ușor; a aluneca pe gât]}, Definition{dict='Micul dicționar academic, ediția a II-a', dictType='definitions', year=2010, text=[A se da peste cap, A preceda în spațiu, A frecventa cursurile unei instituții de învățământ]}]}
